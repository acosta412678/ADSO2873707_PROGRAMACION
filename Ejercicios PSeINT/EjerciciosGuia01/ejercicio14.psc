Proceso ejercicio14
	//Pedir el radio de una circunferencia y calcular su longitud//
	definir diametro como Real;
	definir radio como real;
	Definir longitudCircunferencia como real;
	escribir "escriba el radio de la circunferencia";
	leer radio;
	diametro <- 2 * radio;
	longitudCircunferencia <- diametro * PI;
	escribir "la longitud del circulo con radio ",radio, " es igual a " ,longitudCircunferencia;
	
	

FinProceso
